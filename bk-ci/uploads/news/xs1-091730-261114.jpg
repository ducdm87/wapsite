<script>
//SETTING UP OUR POPUP
//0 means disabled; 1 means enabled;
var popupStatus = 0;

//Press Escape event!
$(document).keypress(function(e){
    if(e.keyCode==27 && popupStatus==1){
        news_detail.disablePopup();
    }
});

$(document).on('keyup','#pop_email #sender_email',function(){
    var email = $.trim($(this).val()),fullname;
    var posat = email.indexOf('@', 0);
    if(posat > -1){
        fullname = email.substring(0, posat);
    }else{
        fullname = email;
    }
    $('#pop_email').find('#sender_name').val(fullname);

});

var news_detail = {
    other_article_id: 0,
    other_limit: 0,
    othernews_next: function(param){
        var box = $("#othernews_paging");
        // get article_id && limit 
        var artical_id = (this.other_article_id == 0) ? $("#article_id").val() : this.other_article_id;
        var limit = (this.other_limit == 0) ? $("#limit_other_news").val() : this.other_limit;
        // next event
        
        box.find(".NextPage_Folder").live("click",function(){
            box.find(".show_other_news").empty();
            $("#load").show();
            var param = {
                flagExc: $(this).attr("data-flagex"),
                intCate: $(this).attr("data-cateid"),
                intThumb: $(this).attr("data-numberthumb"),
                intPage: parseInt($(this).attr("data-page"))+1,
                intLimit: limit,
                article_id: artical_id,
                offset: $(this).attr("data-offset")
            };
            // post data
            $.post(base_url+'/detail/othernews', param, function(data) {
                box.find(".show_other_news").html(data.html);
                $("#load").hide();
            },"json");
        }); 
        // prev event
        box.find(".prvPage_Folder").live("click",function(){
            box.find(".show_other_news").empty();
            $("#load").show();
            var param = {
                flagExc: $(this).attr("data-flagex"),
                intCate: $(this).attr("data-cateid"),
                intThumb: $(this).attr("data-numberthumb"),
                intPage: parseInt($(this).attr("data-page")) - 1,
                article_id: artical_id,
                offset: $(this).attr("data-offset"),
                intLimit: limit
            }
            // post data
            $.post(base_url+'/detail/othernews', param, function(data) {
                box.find(".show_other_news").html(data.html);
                $("#load").hide();
            },"json");
        });
    },
    loadPopup: function (){
        $("#backgroundPopup").css({opacity:"0.7"});
        $("#backgroundPopup").fadeIn("slow");
        $("#popup_email").fadeIn("slow");
        $("#popup_email #pop_email #sender_email").focus();
        //load captcha
        news_detail.loadCaptchaDetail();
        popupStatus = 1;
    },
    disablePopup: function (){
        $("#backgroundPopup").fadeOut("slow");
        $("#popup_email").fadeOut("slow");
        popupStatus = 0;
    },
    submitEmail: function(){
        //alert(1);
        var link = window.location.href;
        $("#url").val(link);

        var a=RegExp(/^(("[\w-+\s]+")|([\w-+]+(?:\.[\w-+]+)*)|("[\w-+\s]+")([\w-+]+(?:\.[\w-+]+)*))(@((?:[\w-+]+\.)*\w[\w-+]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][\d]\.|1[\d]{2}\.|[\d]{1,2}\.))((25[0-5]|2[0-4][\d]|1[\d]{2}|[\d]{1,2})\.){2}(25[0-5]|2[0-4][\d]|1[\d]{2}|[\d]{1,2})\]?$)/i),b= $.trim($("#sender_email").val());
        if(""==b)return alert("Nh\u1eadp Email c\u1ee7a b\u1ea1n"),$("#sender_email").focus(),!1;if(!a.test(b))return alert("Email c\u1ee7a b\u1ea1n kh\u00f4ng h\u1ee3p l\u1ec7"),$("#sender_email").focus(),!1;
        var c=$("#sender_name").val();
        if(""==c)return alert("Nh\u1eadp t\u00ean c\u1ee7a b\u1ea1n"),$("#sender_name").focus(),!1;
        var d=$("#receiver_mail").val();
        if(""==d)return alert("Nh\u1eadp email g\u1eedi \u0111\u1ebfn"),$("#receiver_mail").focus(),!1;
        for(var e=d.split(";"), f=!0,g=0;g<e.length;g++)f=f&&a.test($.trim(e[g]));
            if(!f)return alert("Email g\u1eedi \u0111\u1ebfn kh\u00f4ng h\u1ee3p l\u1ec7"),$("#receiver_mail").focus(),!1;
        a=$("#title_email").val();
        if(""==a)return alert("Nh\u1eadp ti\u00eau \u0111\u1ec1 g\u1eedi mail"),$("#title_email").focus(),!1;
        e=$("#message_mail");
        f=$.trim(this.strip_tags(e.val()));
        e.val(f);
        //news_detail.disablePopup();
        
        var txtCode = $.trim($("#txtCode").val());
        if ($('#txtCode').is(":visible") && txtCode == "") {
            alert("Bạn chưa nhập mã bảo mật");
            $("#txtCode").focus();
            return false;
        }
        
        $.ajax({
            type: "POST",
            url: base_url + "/detail/sendmail",
            data: {
                sender_name: c,
                email_user: b,
                title_email: a,
                receiver_mail: d,
                message_mail: f,
                url: $("#url").val(),
                txtCode     : txtCode,
                captchaID   : $('#captchaID').val()
            },
            beforeSend: function () {
            },
            success: function (a) {
                if($.trim(a)=='done'){
                    alert('Gửi bài viết qua Email thành công. Cảm ơn bạn.');
                    news_detail.disablePopup();
                }
                else if(a=='captcha'){
                    alert('Mã bảo mật chưa chính xác. Mời bạn nhập lại.');
                    //load captcha
                    news_detail.loadCaptchaDetail();
                    return false;    
                }
                else{
                    alert('Không gửi được email. Xin vui lòng thử lại sau.');
                    news_detail.disablePopup();
                }
            }
        });

    },
    strip_tags: function(a,b){b=(((b||"")+"").toLowerCase().match(/<[a-z][a-z0-9]*>/g)||[]).join("");return a.replace(/\x3c!--[\s\S]*?--\x3e|<\?(?:php)?[\s\S]*?\?>/gi,"").replace(/<\/?([a-z][a-z0-9]*)\b[^>]*>/gi,function(a,d){return-1<b.indexOf("<"+d.toLowerCase()+">")?a:""})
    },
    loadCaptcha: function(){
        $.ajax({
            type:'GET',
            url:base_url + '/captcha/show',
            dataType:'json',
            beforeSend: function(){},
            success:function(response){
                if(response != false)
                {
                    $('#showCaptcha').html(response.html);
                }
            }
        });
    },
    loadCaptchaDetail: function(){
        $.ajax({
            type:'GET',
            url:base_url + '/captcha/show',
            dataType:'json',
            beforeSend: function(){},
            success:function(response){
                if(response != false)
                {
                    $('#showCaptchaDetail').html(response.html);
                }
            }
        });
    }
    
}
